package com.globits.mtyt.domain;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;
import javax.xml.bind.annotation.XmlRootElement;

import com.globits.core.domain.BaseObject;

@Entity
@Table(name = "tbl_chomeo")
@XmlRootElement
public class ChoMeo extends BaseObject {
	@Column(name="number_of_feet")
	private int numberOfFeet;
	@Column(name="ages")
	private int ages;
	@Column(name="weight")
	private float weight;
	public int getNumberOfFeet() {
		return numberOfFeet;
	}
	public void setNumberOfFeet(int numberOfFeet) {
		this.numberOfFeet = numberOfFeet;
	}
	public int getAges() {
		return ages;
	}
	public void setAges(int ages) {
		this.ages = ages;
	}
	public float getWeight() {
		return weight;
	}
	public void setWeight(float weight) {
		this.weight = weight;
	}
	

}
